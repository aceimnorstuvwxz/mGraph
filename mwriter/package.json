{
  "name": "Langedit",
  "version": "1.0.0",
  "productName": "Langedit",
  "description": "Open Source Web service monitor, web content change monitor",
  "main": "main.js",
  "build": {
    "appId": "com.boringuniverse.Langedit",
    "mac": {
      "category": "public.app-category.productivity"
