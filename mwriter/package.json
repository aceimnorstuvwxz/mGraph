    "monaco-editor": "^0.11.1",
    "printf": "^0.2.5",
    "request": "^2.83.0",
    "sqlite3": "^3.1.13",
    "superagent": "^3.8.2",
    "turndown": "^4.0.1",
    "uuid": "^3.1.0",
    "word-count": "^0.2.2"
